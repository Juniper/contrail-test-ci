[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[project]
[access "refs/heads/*"]
	label-Approved = +0..+1 group contrail-test-ci-core
	label-Code-Review = -2..+2 group contrail-test-ci-core
	workInProgress = group contrail-test-ci-core
[access "refs/heads/R3.0"]
	exclusiveGroupPermissions = label-Approved
	label-Approved = +0..+1 group Release Managers
[access "refs/tags/*"]
	create = group contrail-test-ci-core
	pushTag = group contrail-test-ci-core
[access "refs/heads/R3.0.2.x"]
	exclusiveGroupPermissions = label-Approved
	label-Approved = +0..+1 group Release Twig Managers
[access "refs/heads/R3.1"]
	exclusiveGroupPermissions = label-Approved
	label-Approved = +0..+1 group Release Managers
[access "refs/heads/R3.0.3.x"]
	exclusiveGroupPermissions = label-Approved
	label-Approved = +0..+1 group Release Twig Managers
[access "refs/heads/R3.1.1.x"]
	exclusiveGroupPermissions = label-Approved
	label-Approved = +0..+1 group Release Twig Managers
[access "refs/heads/master"]
	exclusiveGroupPermissions = label-Approved
	label-Approved = +0..+1 group Read-only
